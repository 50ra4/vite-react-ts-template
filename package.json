{
  "name": "vite-react-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:prettier": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint:eslint": "eslint --fix 'src/**/*.{js,ts,tsx}'",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "check-type": "tsc --noEmit",
    "test": "jest -c ./jest.config.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "cp gh-pages/manager-head.html .storybook && BASE_PATH=/vite-react-ts-template/storybook/ build-storybook -s public -o docs/storybook && rm -f .storybook/manager-head.html",
    "generate-gh-pages": "BASE_PATH=/vite-react-ts-template/ npm run build && rm -rf docs && cp -r dist docs && npm run build-storybook && cp gh-pages/404.html docs"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "@vitejs/plugin-react": "2.1.0",
    "babel-loader": "8.2.5",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-react": "7.31.9",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.5",
    "eslint-plugin-testing-library": "5.7.2",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.6.4",
    "vite": "3.1.0"
  },
  "author": "50ra4",
  "license": "MIT",
  "description": "React + TypeScript development template using Vite",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/50ra4/vite-react-ts-template.git"
  },
  "bugs": {
    "url": "https://github.com/50ra4/vite-react-ts-template/issues"
  },
  "homepage": "https://github.com/50ra4/vite-react-ts-template#readme"
}
